@model SuporteCore.Entity.Analise

@{
    ViewData["Title"] = "Edit";
}

<h1>Adicionar Ocorrencia</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AnaliseId" />
            <div class="form-group">
                <label asp-for="NomeRazao" class="control-label"></label>
                <input asp-for="NomeRazao" class="form-control" />
                <span asp-validation-for="NomeRazao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CpfCnpj" class="control-label"></label>
                <input asp-for="CpfCnpj" class="form-control" />
                <span asp-validation-for="CpfCnpj" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nsu" class="control-label"></label>
                <input asp-for="Nsu" class="form-control" />
                <span asp-validation-for="Nsu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Card_number" class="control-label"></label>
                <input asp-for="Card_number" class="form-control" />
                <span asp-validation-for="Card_number" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Terminal" class="control-label"></label>
                <input asp-for="Terminal" class="form-control" />
                <span asp-validation-for="Terminal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Confirmation_date" class="control-label"></label>
                <input asp-for="Confirmation_date" class="form-control" />
                <span asp-validation-for="Confirmation_date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Date_base" class="control-label"></label>
                <input asp-for="Date_base" class="form-control" />
                <span asp-validation-for="Date_base" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Obsservacao" class="control-label"></label>
                <input asp-for="Obsservacao" class="form-control form-control-lg">
                <span asp-validation-for="Obsservacao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-outline-primary" /> | <a asp-action="Index" class="btn btn-outline-danger">Voltar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
